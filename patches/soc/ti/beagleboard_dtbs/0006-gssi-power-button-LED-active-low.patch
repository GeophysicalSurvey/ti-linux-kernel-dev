From 436ea7ec1027cf93931fb938280df52ad01f49fa Mon Sep 17 00:00:00 2001
From: Jeremy Stashluk <stashlukj@geophysical.com>
Date: Tue, 3 Nov 2020 13:21:47 -0500
Subject: [PATCH] gssi: power button LED active low

---
 arch/arm/boot/dts/gssi-27-009-gpio.dtsi | 50 ++++++++++++-------------
 1 file changed, 25 insertions(+), 25 deletions(-)

diff --git a/arch/arm/boot/dts/gssi-27-009-gpio.dtsi b/arch/arm/boot/dts/gssi-27-009-gpio.dtsi
index 072dc755ac8a..1b6e0efe7e66 100644
--- a/arch/arm/boot/dts/gssi-27-009-gpio.dtsi
+++ b/arch/arm/boot/dts/gssi-27-009-gpio.dtsi
@@ -1,38 +1,38 @@
 / {
-	gpio_keys {
-		compatible = "gpio-keys";
+    gpio_keys {
+        compatible = "gpio-keys";
 
-		marker {
-			label = "GSSI Marker Switch";
-		    pinctrl-names = "default";
-		    pinctrl-0 = <&power_switch_pins>;
-			gpios = <&gpio2 31 GPIO_ACTIVE_HIGH>;
-			debounce-interval = <5>;
-			interrupts = <0x1 0x4 0x7>;
-		};
-	};
+        marker {
+            label = "GSSI Marker Switch";
+            pinctrl-names = "default";
+            pinctrl-0 = <&power_switch_pins>;
+            gpios = <&gpio2 31 GPIO_ACTIVE_HIGH>;
+            debounce-interval = <5>;
+            interrupts = <0x1 0x4 0x7>;
+        };
+    };
 
-	gpio_poweroff {
-		compatible = "gpio-poweroff";
-		gpios = <&gpio1 31 GPIO_ACTIVE_HIGH>;
-	};
+    gpio_poweroff {
+        compatible = "gpio-poweroff";
+        gpios = <&gpio1 31 GPIO_ACTIVE_HIGH>;
+    };
 
-	sir4k_cable {
-		compatible = "extcon-gpio";
-		gpios = <&gpio2 20 GPIO_ACTIVE_HIGH>;
-	};
+    sir4k_cable {
+        compatible = "extcon-gpio";
+        gpios = <&gpio2 20 GPIO_ACTIVE_HIGH>;
+    };
 
     power_leds {
-		compatible = "gpio-leds";
+        compatible = "gpio-leds";
 
-		pinctrl-names = "default";
-		pinctrl-0 = <&power_switch_pins>;
+        pinctrl-names = "default";
+        pinctrl-0 = <&power_switch_pins>;
 
         power_led0 {
             label = "gssi:led2_7";
-		    gpios = <&gpio2 7 GPIO_ACTIVE_HIGH>;
-			linux,default-trigger = "default-on";
-		    default-state = "on";
+            gpios = <&gpio2 7 GPIO_ACTIVE_LOW>;
+            linux,default-trigger = "default-on";
+            default-state = "on";
         };
     };
 };
-- 
